{
  "tasks": {
    "dev": "deno run -A --unstable-kv npm:vite",
    "build": "deno run -A --unstable-kv npm:vite build",
    "preview": "deno run -A --unstable-kv npm:vite preview",
    "serve": "deno run -A --unstable-kv build/index.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@eslint/compat": "npm:@eslint/compat@^1.2.8",
    "@eslint/js": "npm:@eslint/js@^9.24.0",
    "@sveltejs/adapter-node": "npm:@sveltejs/adapter-node@^5.2.12",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.20.4",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.3",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.3",
    "daisyui": "npm:daisyui@^5.0.15",
    "eslint": "npm:eslint@^9.24.0",
    "eslint-config-prettier": "npm:eslint-config-prettier@^10.1.1",
    "eslint-plugin-svelte": "npm:eslint-plugin-svelte@^3.5.1",
    "globals": "npm:globals@^16.0.0",
    "prettier": "npm:prettier@^3.5.3",
    "prettier-plugin-svelte": "npm:prettier-plugin-svelte@^3.3.3",
    "prettier-plugin-tailwindcss": "npm:prettier-plugin-tailwindcss@^0.6.11",
    "svelte": "npm:svelte@^5.25.8",
    "svelte-check": "npm:svelte-check@^4.1.5",
    "sveltekit-sse": "npm:sveltekit-sse@^0.13.16",
    "tailwindcss": "npm:tailwindcss@^4.1.3",
    "typescript-eslint": "npm:typescript-eslint@^8.29.1",
    "vite": "npm:vite@^6.2.5",
    "zod": "npm:zod@^3.24.2"
  },
  "exclude": [".svelte-kit/"],
  "nodeModulesDir": "auto"
}
